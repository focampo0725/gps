<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item android:right="0dp">
                <shape android:shape="oval">
                    <solid android:color="#E2B93C"/>
                </shape>
            </item>
            <item android:top="1dp" android:left="1dp" android:right="0.6dp">
                <shape android:shape="oval">
                    <solid android:color="#000000" />
                </shape>
            </item>
            <item android:right="3dp" android:bottom="2dp" >
                <shape android:shape="oval">
                    <gradient
                        android:angle="90"
                        android:startColor="#FFB833"
                        android:centerColor="#FFC64D"
                        android:useLevel="true"
                        android:endColor="#FFD873" />
                </shape>
            </item>
            <item
                android:drawable="@drawable/ic_information_32" android:gravity="center" />
        </layer-list>
    </item>
</selector>
